{
  "name": "react-minesweeper",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "4.5.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "build-css": "node-sass src/base.scss -o src/styles/",
    "run-css": "yarn build-css && node-sass src/base.scss -o src/styles/ --watch --recursive",
    "start": "npm-run-all -p run-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
